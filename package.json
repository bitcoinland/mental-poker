{
  "name": "poker-js",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm:build:react\" \"npm:start:node\"",
    "start-dev": "concurrently \"npm:start:react\" \"npm:start:node\"",
    "start:node": "node src/app.js",
    "start:react": "NODE_ENV=dev webpack --config ./src/react/webpack.config.js --mode development --info-verbosity verbose --progress",
    "start:angular": "browserify src/public/js/angular/controllers/table.js -o src/public/js/angular/controllers/table-bundle.js && node src/app.js",
    "debug:angular": "browserify src/public/js/angular/controllers/table.js -o src/public/js/angular/controllers/table-bundle.js && node inspect src/app.js",
    "test": "mocha",
    "test-debug": "mocha debug",
    "build:react": "NODE_ENV=production webpack -p --config ./src/react/webpack.config.js --mode production"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
