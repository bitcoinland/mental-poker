    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "js-cookie": "^2.2.0",
    "js-sha256": "~0.9.0",
    "jsonwebtoken": "^8.5.1",
    "less": "~3.8.1",
    "less-middleware": "~3.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.shuffle": "^4.2.0",
